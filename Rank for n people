#include <iostream>
#include <vector>
#include <unordered_map>
#include <unordered_set>
#include <math.h>
#include <string>
using namespace std;

long long solveranking(int n)
{
    long long dp[n+1][n+1];     // # of ranking(名次), # of people
    long long fact[n+1];
    
    fact[1] = 1;
    for(int i=2;i<=n;i++)
        fact[i] = i*fact[i-1];
    
    for(int i=1;i<=n;i++)
        dp[1][i] = 1;
    
    for(int i=1;i<=n;i++)
    {
        for(int j=i;j<=n;j++)
        {
            if(i==j)
                dp[i][j] = fact[i];
            else
                dp[i][j] = i*dp[i][j-1]+ i*dp[i-1][j-1];
        }
    }
    
    long long result=0;
    
    for(int i=1;i<=n;i++)
        result += dp[i][n];
    return result;
}

int main(int argc, const char * argv[]) {

    int n=3;
    cout<<solveranking(n)<<endl;
    return 0;
}


