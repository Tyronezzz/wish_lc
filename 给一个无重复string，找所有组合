vector<string> findcombination(string str)
{
    vector<string> result;
    
    if(str.size()==0) return result;
    
    for(int i=0;i<str.size();i++)
    {
        for(int j=i;j<str.size();j++)
        {
            result.push_back(str.substr(i, j-i+1));
        }
        
    }
    return result;
    
}
